{
   "background": {
      "persistent": true,
      "scripts": [ "js/jquery/jquery.js", "js/js/xml2json.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "notification.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "css/jquery-ui.css", "css/jquery-ui.structure.css", "css/jquery-ui.theme.css", "css/toastr.min.css", "css/glyphicons.css", "css/global.css" ],
      "js": [ "js/jquery/jquery.js", "js/jquery/jquery-ui.js", "js/misc.js", "js/js/mousetrap.min.js", "js/js/mousetrap-record.min.js", "js/js/toastr.min.js", "js/js/arrive.min.js", "js/js/vue.min.js", "js/main.js" ],
      "matches": [ "*://*.kissanime.ru/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "css": [ "css/homepage.css" ],
      "js": [ "js/js/jquery17.min.js", "js/js/jqueryTooltip.js", "js/homepage.js" ],
      "matches": [ "*://*.kissanime.ru/" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "js/AnimeInfo.js" ],
      "matches": [ "*://*.kissanime.ru/Anime/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": false,
      "css": [ "css/videopage.css" ],
      "js": [ "js/js/jquery.inputmask.bundle.js", "js/videopage.js" ],
      "matches": [ "*://*.kissanime.ru/Anime/*/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "js/bookmarkspage.js" ],
      "matches": [ "*://*.kissanime.ru/BookmarkList*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "js/jquery/jquery.js", "js/videoHosts/openload.js" ],
      "matches": [ "*://openload.co/embed/*", "*://oload.stream/embed/*", "*://oload.site/embed/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "js/videoHosts/rapidvideo.js" ],
      "matches": [ "*://*.rapidvideo.com/e/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "js/videoHosts/streamango.js" ],
      "matches": [ "*://*.streamango.com/embed/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": false,
      "js": [ "js/jquery/jquery.js", "js/MAL2KissAnime.js" ],
      "matches": [ "*://myanimelist.net/anime/*", "*://myanimelist.net/anime.php?id=*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "css": [ "css/disqus.css" ],
      "js": [ "js/jquery/jquery.js", "js/disqus.js" ],
      "matches": [ "*://disqus.com/embed/comments/?base=default&f=kissanime*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Adds the ability to toggle on/off certain elements on KissAnime as well as adding new features.",
   "icons": {
      "128": "icon128.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "45.0.2454",
   "name": "Essentials for KissAnime",
   "options_page": "options.html",
   "options_ui": {
      "chrome_style": false,
      "open_in_tab": true,
      "page": "options.html"
   },
   "permissions": [ "\u003Call_urls>", "activeTab", "storage", "notifications", "webRequest", "webRequestBlocking" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2019.1.14.1",
   "version_name": "v2019.1.14.1",
   "web_accessible_resources": [ "fonts/*", "images/*", "InjectedHTML/*", "assets/*", "css/global.css", "js/*", "options.html" ]
}
